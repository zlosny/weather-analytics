<coordinator-app name="ExecSqoopJobCoord" frequency="${coord:days(1)}"
                 start="${startTime}" end="${endTime}" timezone="${timeZone}"
                 xmlns="uri:oozie:coordinator:0.1">
    <action>
        <workflow>
            <app-path>${workflowPath}</app-path>
            <configuration>
                <property>
                    <name>date</name>
                    <value>${coord:formatTime(coord:nominalTime(), 'yyyyMMdd')}</value>
                </property>
            </configuration>
        </workflow>
    </action>
</coordinator-app>
